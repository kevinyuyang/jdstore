      <li data-target="#myCarousel" data-slide-to="3"></li>
    </ol>

    <!-- Wrapper for slides -->
    <div class="carousel-inner" role="listbox">

      <div class="item active">
        <a href="/products">
        <img src="https://ww1.sinaimg.cn/large/006tNbRwly1fcwzx1tdrcj30zk0sj11m.jpg" alt="Chania" width="460" height="345">
        </a>
        <div class="carousel-caption">
          <h3>DYLAN</h3>
